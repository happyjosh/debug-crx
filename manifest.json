{
  "manifest_version": 3,
  "name": "Pionex-Tool",
  "description": "Pionex tools",
  "version": "1.0",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "/dist/popup/index.html",
    "default_title": "Pionex-Tool"
  },
  "host_permissions": ["<all_urls>"],
  "permissions": [
    "contextMenus",
    "tabs",
    "cookies",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/dist/bundle.js"],
      "css": ["custom.css"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["/dist/bundle.js"],
      "matches": [],
      "extension_ids": []
    }
  ]
}
